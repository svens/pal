constexpr std::string_view pkcs12 = //{{{1
/* Note the order of PEMs, tests expect importing to re-order.
cat server.key.pem client.pem intermediate.pem ca.pem server.pem \
  | openssl pkcs12 -export -passin pass:ServerPassword -passout pass:TestPassword \
  | openssl base64 \
  | pbcopy
*/
	"MIIcqQIBAzCCHG8GCSqGSIb3DQEHAaCCHGAEghxcMIIcWDCCFw8GCSqGSIb3DQEH"
